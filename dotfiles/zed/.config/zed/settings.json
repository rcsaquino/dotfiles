{
  "agent": {
    "model_parameters": [],
    "default_model": {
      "provider": "google",
      "model": "gemini-2.0-flash"
    },
    "version": "2"
  },
  "languages": {
    "Odin": {
      "show_edit_predictions": false
    }
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  },

  "autosave": { "after_delay": { "milliseconds": 500 } },
  "terminal": { "toolbar": { "breadcrumbs": false } },
  "restore_on_startup": "none",
  "auto_signature_help": true,
  "extend_comment_on_newline": false
}
